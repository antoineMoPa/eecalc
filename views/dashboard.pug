.dashboard-content
  col-md-6
    h3 Dashboard
    h4 Recently visited sheets
    .recent-sheet-list
    ul
      for sheet in recent_sheets
        -url = "/sheet/"+sheet
        li
          a(href=url)= url
  col-md-6
