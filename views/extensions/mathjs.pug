template(name="mathjs-cell-type")
  .math-part
    fieldset
      div
        input.inline-block.livecalc-input.livecalc-focus-element(
          type='text', 
          placeholder='Type math')
        button.inline-block.livecalc-go-button
          | Go
    label Your input (Pretty print)
    .katex-input
    col-md-6
      label Result (Pretty print)
      .katex-output
    col-md-6
      label Result (Raw ouput)
      pre.livecalc-output
      pre.livecalc-secondary-output
    clearfix
    col-sm-6
      p.cell-state.text-left
    col-sm-6
      p.users-info.text-right
    clearfix
    .plot
